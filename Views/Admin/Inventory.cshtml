@model Car_delearship.Models.ViewModel.AddEditVehicleViewModel
    ViewData["Title"] = "Inventory";
    Layout = null;
}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>

<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <div class="container-fluid">
        <a class="navbar-brand" href="www.lisautosales.co.za">Lis Auto Sales</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="@Url.Action("Dashboard")">Dashboard</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Makes")">Makes</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Models")">Models</a>
                </li>
            </ul>
        </div>
    </div>
</nav>

<br />
<br />
<br />
<div>
    <table class="table table-striped settings-table sortable"   data-show-pagination-switch="true"
  data-pagination="true"
  data-id-field="id"
  data-page-list="[10, 25, 50, 100, all]"
  data-show-footer="true"
  data-side-pagination="server" style="width: 100%; height:auto;">
        <thead>
            <tr>
                <th scope="col">Item No</th>
                <th scope="col">Make</th>
                <th scope="col">Model</th>
                <th scope="col">Milage</th>
                <th scope="col">In Stock</th>
            </tr>
        </thead>
        @foreach (var iten in Model.Vehicles)
        {
       <tbody>
        <tr>
            <th scope="row">@Html.DisplayFor(modelitem => iten.VehicleID)</th>
            <td>BMW</td>
            <td>3 series</td>
            <td>12000 Km</td>
            <td>10 days ago</td>

        </tr>
    </tbody>
}
    </table>
</div>